@model List<Faq>
<div class="container pt-5">
    @foreach (Faq item in Model)
    {
    <div class="accordion accordion-flush" id="accordionFlushExample">
        <div class="accordion-item">
            <h2 class="accordion-header" id="flush-headingOne">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapse-@item.Id" aria-expanded="false" aria-controls="flush-collapseOne">
                    @item.Question
                </button>
            </h2>
            <div id="flush-collapse-@item.Id" class="accordion-collapse collapse ps-3" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample">
                    @item.Answer
            </div>
        </div>
        

    </div>

    }
</div>